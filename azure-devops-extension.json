{
  "manifestVersion": 1,
  "id": "azure-game-ops",
  "publisher": "sharpenedkey",
  "version": "0.0.21",
  "name": "Azure GameOps",
  "description": "Slack off at work while making it look like you're working! What more could you want?",
  "public": false,
  "categories": ["Azure Repos"],
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "icons": {
    "default": "resources/logo.png"
  },
  "content": {
    "details": {
      "path": "overview.md"
    }
  },
  "files": [
    {
      "path": "resources",
      "addressable": true
    },
    {
      "path": "dist",
      "addressable": true
    }
  ]
}
